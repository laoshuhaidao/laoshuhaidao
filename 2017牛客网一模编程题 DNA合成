#include<iostream>
#include<string>
using namespace std;

int main()
{
	string s1, s2;
	int num = 0;
	cin >> s1 >> s2;
	for (int i = 0; i < s1.length(); i++)
	{
		if ((s1[i] == 'A'&&s2[i] == 'T') || (s1[i] == 'T'&&s2[i] == 'A') || (s1[i] == 'C'&&s2[i] == 'G') || (s1[i] == 'G'&&s2[i] == 'C'))
			continue;
		else num++;
	}
	cout << num;
	return 0;
}
